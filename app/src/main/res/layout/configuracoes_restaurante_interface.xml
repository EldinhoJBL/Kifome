<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EB1B23"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="27dp"
        android:layout_height="35dp"
        android:clickable="true"
        android:focusable="true"
        android:onClick="CardapioClienteInterface1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.008">

        <ImageView
            android:id="@+id/imageView35"
            android:layout_width="31dp"
            android:layout_height="25dp"
            android:importantForAccessibility="no"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.444"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/captura_de_tela_2025_03_30_230939"
            tools:ignore="TouchTargetSizeCheck,VisualLintBounds" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="493dp"
        android:layout_height="116dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_074914"
        tools:layout_editor_absoluteX="-68dp"
        tools:layout_editor_absoluteY="726dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="117dp"
        android:layout_height="57dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_075043"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="742dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="95dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_27_003637"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="741dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="108dp"
        android:layout_height="57dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_075142"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="742dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="221dp"
        android:layout_height="39dp"
        android:text="Notificações:"

        android:textColor="#FAFAFA"

        android:textSize="24sp"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="140dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="226dp"
        android:layout_height="34dp"
        android:text="Versão Atual: X.X.X"

        android:textColor="#FAFAFA"

        android:textSize="20sp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="671dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="105dp"
        android:layout_height="48dp"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="133dp" />
</androidx.constraintlayout.widget.ConstraintLayout>