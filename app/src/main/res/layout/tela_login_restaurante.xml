<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EB1B23"
    tools:context=".ConfiguracoesClienteInterface1">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="311dp"
        android:layout_height="161dp"
        android:layout_marginHorizontal="16dp"
        android:layout_marginTop="4dp"
        android:adjustViewBounds="true"
        android:contentDescription="Logo do aplicativo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.8"
        app:srcCompat="@drawable/captura_de_tela_2025_03_26_015257"
        tools:ignore="HardcodedText,ImageContrastCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="345dp"
        android:layout_height="237dp"
        android:src="@drawable/captura_de_tela_2025_05_28_222224"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="501dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="362dp"
        android:layout_height="135dp"
        android:src="@drawable/captura_de_tela_2025_05_28_222152"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="225dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="374dp"
        android:layout_height="154dp"
        android:src="@drawable/captura_de_tela_2025_05_28_222213"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="142dp"
        android:layout_height="20dp"
        android:background="#00D9A60D"
        android:backgroundTint="#B4942A"
        android:drawableTint="#DB0B0B"
        android:filterTouchesWhenObscured="true"
        android:fontFamily="@font/alata"
        android:foregroundTint="#EDD400"
        android:text="Esqueci minha senha"
        android:textColor="#FFC107"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="474dp" />

</androidx.constraintlayout.widget.ConstraintLayout>