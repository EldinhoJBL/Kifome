        android:textSize="24sp"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="221dp"
        android:layout_height="39dp"
