<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EB1B23"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="149dp"
        android:layout_height="68dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@drawable/captura_de_tela_2025_03_26_015257"
        tools:ignore="ImageContrastCheck,MissingConstraints,VisualLintBounds"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="493dp"
        android:layout_height="116dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_074914"
        tools:layout_editor_absoluteX="-70dp"
        tools:layout_editor_absoluteY="705dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="117dp"
        android:layout_height="57dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_075043"
        tools:layout_editor_absoluteX="129dp"
        tools:layout_editor_absoluteY="721dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="95dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_27_003637"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="720dp" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="108dp"
        android:layout_height="57dp"
        app:srcCompat="@drawable/captura_de_tela_2025_03_29_075142"
        tools:layout_editor_absoluteX="246dp"
        tools:layout_editor_absoluteY="721dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="221dp"
        android:layout_height="39dp"
        android:text="Notificações:"

        android:textColor="#FAFAFA"

        android:textSize="24sp"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="140dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="226dp"
        android:layout_height="34dp"
        android:text="Versão Atual: X.X.X"

        android:textColor="#FAFAFA"

        android:textSize="20sp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="671dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="105dp"
        android:layout_height="48dp"
        tools:layout_editor_absoluteX="185dp"
        tools:layout_editor_absoluteY="133dp" />


</androidx.constraintlayout.widget.ConstraintLayout>